{
	"name": "react-islands-examples",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev:client": "vite --config vite.client.config.js",
		"build:client": "node scripts/build.mjs",
		"dev:contentstack-demo": "cross-env NODE_ENV=development nodemon --watch contentstack-demo --ext js,jsx --exec \"node --loader @esbuild-kit/esm-loader contentstack-demo/server/index.js\"",
		"dev:commercetools-demo": "cross-env NODE_ENV=development nodemon --watch commercetools-demo --ext js,jsx --exec \"node --loader @esbuild-kit/esm-loader commercetools-demo/server/index.js\"",
		"dev:agility-demo": "cross-env NODE_ENV=development nodemon --watch agility-demo --ext js,jsx --exec \"node --loader @esbuild-kit/esm-loader agility-demo/server/index.js\"",
		"dev:contentstack-commercetools-demo": "cross-env NODE_ENV=development nodemon --watch contentstack-commercetools-demo --ext js,jsx --exec \"node --loader @esbuild-kit/esm-loader contentstack-commercetools-demo/server/index.js\"",
		"dev:contentstack": "cross-env DEMO_TARGET=contentstack node scripts/dev-run.mjs",
		"dev:commercetools": "cross-env DEMO_TARGET=commercetools node scripts/dev-run.mjs",
		"dev:agility": "cross-env DEMO_TARGET=agility node scripts/dev-run.mjs",
		"dev:contentstack-commercetools": "cross-env DEMO_TARGET=contentstack-commercetools node scripts/dev-run.mjs"
	},
	"dependencies": {
		"@commercetools/platform-sdk": "^8.24.0",
		"@commercetools/ts-client": "^3.2.0",
		"contentstack": "^3.26.4",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"node-fetch": "^3.3.2",
		"path-to-regexp": "^6.2.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-islands-runtime": "file:../builds/react-islands-runtime-0.1.22.tgz"
	},
	"devDependencies": {
		"@esbuild-kit/esm-loader": "^2.6.5",
		"@vitejs/plugin-react": "^4.3.4",
		"cross-env": "^10.1.0",
		"esbuild": "^0.27.3",
		"glob": "^13.0.2",
		"nodemon": "^3.1.0",
		"vite": "^6.1.0"
	}
}
